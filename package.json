{
  "name": "open-duck",
  "version": "1.0.0",
  "private": true,
  "description": "open-duck",
  "templateInfo": {
    "name": "taro-ui",
    "typescript": true,
    "css": "Sass",
    "framework": "React"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "stylelint": "stylelint \"src/**/*.{css,scss,sass}\"",
    "stylelint:fix": "stylelint \"src/**/*.{css,scss,sass}\" --fix",
    "type": "tsc --noEmit",
    "type:check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,scss,sass,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,css,scss,sass,json,md}\"",
    "check": "pnpm run lint && pnpm run stylelint && pnpm run type && pnpm run test",
    "check:fix": "pnpm run lint:fix && pnpm run stylelint:fix && pnpm run format",
    "check:lint": "pnpm run lint && pnpm run stylelint",
    "check:lint:fix": "pnpm run lint:fix && pnpm run stylelint:fix",
    "check:type": "pnpm run type",
    "check:test": "pnpm run test",
    "check:format": "pnpm run format:check",
    "check:format:fix": "pnpm run format",
    "prepare": "husky install"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "dependencies": {
    "@babel/runtime": "^7.21.5",
    "@tarojs/components": "4.1.5",
    "@tarojs/helper": "4.1.5",
    "@tarojs/plugin-framework-react": "4.1.5",
    "@tarojs/plugin-platform-alipay": "4.1.5",
    "@tarojs/plugin-platform-h5": "4.1.5",
    "@tarojs/plugin-platform-jd": "4.1.5",
    "@tarojs/plugin-platform-qq": "4.1.5",
    "@tarojs/plugin-platform-swan": "4.1.5",
    "@tarojs/plugin-platform-tt": "4.1.5",
    "@tarojs/plugin-platform-weapp": "4.1.5",
    "@tarojs/react": "4.1.5",
    "@tarojs/runtime": "4.1.5",
    "@tarojs/shared": "4.1.5",
    "@tarojs/taro": "4.1.5",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "taro-ui": "^3.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/plugin-transform-object-rest-spread": "^7.28.0",
    "@taro-hooks/plugin-react": "^2.2.0",
    "@tarojs/cli": "4.1.5",
    "@tarojs/plugin-html": "^4.1.5",
    "@tarojs/test-utils-react": "^0.1.1",
    "@tarojs/vite-runner": "4.1.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.1",
    "@types/react": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "@vitejs/plugin-react": "^4.1.0",
    "babel-preset-taro": "4.1.5",
    "eslint": "^8.12.0",
    "eslint-config-taro": "4.1.5",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^8.0.3",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.18",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.6.2",
    "react-refresh": "^0.11.0",
    "sass": "^1.60.0",
    "stylelint": "^14.4.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-scss": "^6.12.1",
    "terser": "^5.16.8",
    "ts-node": "^10.9.2",
    "typescript": "^5.1.0",
    "vite": "^4.2.0"
  }
}
